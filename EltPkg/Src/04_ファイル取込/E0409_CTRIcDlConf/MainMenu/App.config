<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>
  <appSettings>
    <!-- OCR読取フィールド名設定（持帰銀行コード） -->
    <add key="OCRKey_IC_BK_NO" value="持帰銀行"/>
    <!-- OCR読取フィールド名設定（金額） -->
    <add key="OCRKey_AMOUNT" value="金額"/>
    <!-- OCR読取フィールド名設定（交換日） -->
    <add key="OCRKey_CLEARING_DATE" value="交換希望日"/>
    <!-- OCR読取フィールド名設定（手形種類） -->
    <add key="OCRKey_BILL_CODE" value="手形種類"/>
    <!-- OCR読取フィールド名設定（持帰支店コード） -->
    <add key="OCRKey_IC_BR_NO" value="支店番号"/>
    <!-- OCR読取フィールド名設定（口座番号） -->
    <add key="OCRKey_ACCOUNT" value="口座番号"/>
    <!-- OCR読取フィールド名設定（手形番号） -->
    <add key="OCRKey_BILL_NO" value="手形番号"/>
    <!-- QR情報読取設定(手形種類コード) -->
    <add key="QR_SYURUI_CODE" value="10,3"/>
    <!-- QR情報読取設定(手形番号) -->
    <add key="QR_BILL_NO" value="30,10"/>
    <!-- 行内連携での持出OCR信頼度閾値（持出のOCRを有効とする閾値） -->
    <add key="OCOCRLevel" value="10"/>
    <!-- 証券明細テキスト:手形・小切手番号の取得(正規表現[グループ化の箇所を取得]) -->
    <add key="BILLNO_REGEXPATTERN" value="(\d+)$"/>
  </appSettings>
</configuration>
