<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings>
    <!-- リトライ回数 -->
    <add key="RetryCount" value="10"/>
    <!-- スリープタイム（ミリ秒） -->
    <add key="SleepTime" value="1000"/>
  </appSettings>
</configuration>