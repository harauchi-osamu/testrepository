<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>
  <appSettings>
    <!-- イメージファイル削除設定 -->
    <add key="UNMATCH_TRIMGDELETE" value='true'/>
    <!-- 削除定義 【&lt;(<) &gt;(>)】 -->
    <add key="1:BALANCETXT" value='"1"|"-3"|"DELETE FROM {0}.BALANCETXT WHERE EXISTS (SELECT 1 FROM {0}.BALANCETXT_CTL WHERE BALANCETXT_CTL.FILE_NAME = BALANCETXT.FILE_NAME AND BALANCETXT_CTL.MAKE_DATE &lt;= {1})"'/>
    <add key="2:BALANCETXT_CTL" value='"1"|"-3"|"DELETE FROM {0}.BALANCETXT_CTL WHERE BALANCETXT_CTL.MAKE_DATE &lt;= {1}"'/>
    <add key="3:BR_TOTAL" value='"0"|"-3"|"DELETE FROM {0}.BR_TOTAL WHERE BR_TOTAL.OPERATION_DATE &lt;= {1}"'/>
    <add key="FILEDELETE" value='""0"|"3"|"[ScanImage][ImageBackUpRoot]""'/>
  </appSettings>
</configuration>